{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": 'entry'
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "lodash",
    "@babel/syntax-dynamic-import",
    "@babel/proposal-object-rest-spread",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
