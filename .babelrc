{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%"
        },
        "forceAllTransforms": true,
        "useBuiltIns": "entry",
        "corejs": 3
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "lodash",
    "@babel/syntax-dynamic-import",
    "@babel/proposal-object-rest-spread",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
